<%= simple_form_for(@wedding) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :description %>
    <%= f.input :date %>
    <%= f.input :location %>
    <%= f.input :photo %>
    <%= f.input :capacity %>
    <%= f.association :users %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
