<div class="container">

  <h3 class="text-center">
    <span class="section-title">Photo Album</span>
  </h3>

  <%= simple_form_for(@wedding) do |f| %>
    <div class="form-imputs">
      <%= f.input :album_photos, as: :attachinary, label: false %>
    </div>
    <div class="form-actions">
      <%= f.button :submit, value: "Add memories" %>
    </div>
  <% end %>


  <h3><%= @wedding.user.first_name %> & <%= @wedding.spouse_first_name %></h3>
    <%= render 'carousel' %>
</div>
